%
% fridgeContent.pl
%

content(pizza)
content(salmon)
content(caviar)
content(pasta)

showFridgeContent :- 
	output("FRIDGE CONTENT ---------- "),
	showContents,
	output("--------------------------").
		
showContents:- 
 	content( FOOD ),
 	output( model( CATEG, NAME, STATE ) ),
	fail.
showContents.