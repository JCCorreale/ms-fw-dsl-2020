%
% fridgeContent.pl
%

content(pizza)
content(salmon)
content(caviar)
content(pasta)

showFridgeContent :- 
	output("FRIDGE CONTENT ---------- "),
	showResources,
	output("--------------------------").
		
showResources :- 
 	model( CATEG, NAME, STATE ),
 	output( model( CATEG, NAME, STATE ) ),
	fail.
showResources.