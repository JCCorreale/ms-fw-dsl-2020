content(pizza)
content(salmon)
content(caviar)
content(pasta)

showFridgeContents :- 
	output("FRIDGE CONTENT ---------- "),
	showFridgeContent,
	output("--------------------------").
		
showFridgeContent :- 
 	model( CATEG, NAME, STATE ),
 	output( model( CATEG, NAME, STATE ) ),
	fail.
showFridgeContent.